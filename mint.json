{
  "$schema": "https://mintlify.com/schema.json",
  "name": "flatfile.com",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "api": {
    "baseUrl": "https://api.x.flatfile.com",
    "playground": {
      "mode": "simple"
    },
    "auth": {
        "method": "bearer"
    }
  },
  "favicon": "/favicon.png",
  "colors": {
    "primary": "#3B2FC9",
    "light": "#6673FF",
    "dark": "#3B2FC9",
    "background": {
      "dark": "#0f1117",
      "light": "#fff"
    }
  },
  "feedback": {
    "raiseIssue": true,
    "suggestEdit": true
  },
  "modeToggle": {
    "default": "dark"
  },
  "backgroundImage": "/images/background.png",
  "topbarLinks": [
    {
      "name": "Create account",
      "url": "https://dashboard.flatfile.com"
    }
  ],
  "topbarCtaButton": {
    "name": "Sign in",
    "url": "https://dashboard.flatfile.com"
  },

  "anchors": [
    {
      "name": "Blueprint",
      "icon": "compass-drafting",
      "url": "blueprint"
    },
    {
      "name": "Plugins",
      "icon": "plug",
      "url": "plugins"
    },
    {
      "name": "Developer Tools",
      "icon": "brackets-curly",
      "url": "developer-tools"
    },
    {
      "name": "API Reference",
      "icon": "rectangle-terminal",
      "url": "https://flatfile.stoplight.io/docs/v10/"
    }
  ],
  "navigation": [
    {
      "group": "Documentation",
      "pages": ["overview"]
    },
    {
      "group": "Quickstart",
      "pages": ["quickstart/meet-the-workbook", "quickstart/add-data-transformation", 
     "quickstart/submit-action"]
    },
    {
      "group": "Concepts",
      "pages": ["concepts/workbooks", "concepts/spaces",
          "concepts/agents", "concepts/jobs", "concepts/events"]
    },
    {
      "group": "Core Libraries",
      "pages": ["core-libraries/listener",
          "core-libraries/hooks", "core-libraries/api", "core-libraries/flatfile"]
    },
    {
      "group": "Getting Started",
      "pages": ["getting-started/handling-data",
          "getting-started/using-actions", "getting-started/embedding"]
    },
    {
      "group": "Security",
      "pages": ["security/authentication",
          "security/roles-and-permissions"]
    },
    {
      "group": "Advanced",
      "pages": ["advanced/theming", "advanced/guest_sidebar", "advanced/custom-actions", "advanced/anatomy-of-events"]
    },
    {
      "group": "More scenarios",
      "pages": ["use-cases/headless", "use-cases/workflows",
          "use-cases/ephemeral"]
    },
    {
      "group": "Plugins",
      "pages": ["plugins/overview"]
    },
    {
      "group": "Blueprint",
      "pages": ["blueprint/overview", "blueprint/properties", 
          "blueprint/constraints", "blueprint/relationships", "blueprint/access"]
    },
    {
      "group": "Developer Tools",
      "pages": ["developer-tools/overview", "developer-tools/deploying",
          "developer-tools/ci-integration", "developer-tools/environment"]
    }
  ],
  "footerSocials": {
    "github": "https://github.com/FlatFilers",
    "twitter": "https://twitter.com/flatfilers/",
    "linkedin": "https://linkedin.com/company/flatfile",
    "facebook": "https://facebook.com/flatfilers/",
    "discord": "https://discord.gg/jbTtMKseas"
  },
  "analytics": {
    "gtm": {
        "tagId": "GTM-W673X2W"
    }
}
}
